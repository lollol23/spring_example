<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.lollol23.spring.lesson02.dao.UsedGoodsDAO">
 	<!-- use_goods 테이블의 모든행 가져오기 -->
 	
 	<select id="sellectUsedGoodsList" resultType="com.lollol23.spring.lesson02.model.UsedGoods">
 		SELECT
 			`id`
 			,`title`
 			,`price`
 			,`description`
 			,`sellerId`
 			,`createdAt`
 			,`updatedAt`
 		FROM
 			`used_goods`
 	</select>
 </mapper>